jsnormalize {
  input = item+
  item =
    | string -- string
    | space -- space
    | integer -- integer
    | word -- word
    | any -- other

  word = firstchar legalchar+
  firstchar = letter | "_"
  legalchar = letter | digit | "_"

  integer = digit+
  jscomment = "//" (~"\n" any)* "\n"
  comment = jscomment
  space += comment
  string = dq (~dq any)* dq
  dq = "\""
}
